package javaprgms;

public class Patterns {

	public static void main(String[] args) {
		
//======printing 5 lines of stars===
		
		for(int i =0; i<=5;i++)
		{
			
		for(int j =0; j<=5; j++)
		{
		System.out.print("*");
		}
		System.out.println();
		}
		
//============printing left triangle
		for(int i=0;i<=5;i++)
		{
			for(int j=0;j<=i;j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		for(int k=5;k>=0;k--)
		{
			for(int n=5;n<=k;n++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
