
public class ReverseString {

	public static void main(String[] args) {

		String str = "java   programs";
		String str1 = " ";
		String str2 = " ";

		// int length = str.length();

		String[] strr = str.split(" ");

		for(int i=0;i<strr.length;i++)
		{
			String word = strr[i];
			System.out.println(word);
		}
	}

}
