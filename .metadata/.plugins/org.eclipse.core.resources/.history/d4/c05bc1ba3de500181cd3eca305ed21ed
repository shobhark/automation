
public class ReverseString {

	public static void main(String[] args) {

		String str = "java   programs";
		String str1 = " ";
		String str2 = " ";

		// int length = str.length();

		String[] strr = str.split(" ");

		str1 = strr[0];
		str2 = strr[1];
		System.out.println(str1);
		System.out.println(str2);
	}

}
